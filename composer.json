{
  "name": "vinelab/neoeloquent",
  "description": "Laravel wrapper for the Neo4j graph database REST interface",
  "keywords": [
    "laravel",
    "neo4j",
    "graph",
    "database",
    "neoeloquent"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Abed Halawi",
      "email": "abed.halawi@vinelab.com"
    }
  ],
  "require": {
    "php": "^8.0",
    "heydavid713/neo4jphp": "0.1.*",
    "nesbot/carbon": "~1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.3.3",
    "mockery/mockery": "1.0"
  },
  "repositories": [
  ],
  "autoload": {
    "psr-4": {
      "Vinelab\\NeoEloquent\\": "src"
    }
  },
  "autoload-dev": {
    "classmap": [
      "tests/TestCase.php"
    ]
  },
  "minimum-stability": "stable",
  "extra": {
    "laravel": {
      "providers": [
        "Vinelab\\NeoEloquent\\NeoEloquentServiceProvider"
      ]
    }
  }
}
